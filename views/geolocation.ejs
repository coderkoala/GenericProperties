<% extend('./partials/main') %>

<main class="container" data-key="<%= apiKey %>">
  <div class="my-3 p-3 bg-body rounded shadow-sm">
    <h6 class="border-bottom pb-2 mb-2"><b><%= title %></b></h6>
    <div class="d-flex text-muted pt-3">
      <div class="mb-0 small lh-sm border-bottom w-100">
        <small class="text-muted mb-5">&nbsp;Lead UUID :</small><br />
        <div class="d-flex justify-content-between">
          <div class="input-group mb-3">
            <input
              validation="location"
              id="location"
              type="text"
              class="form-control"
              placeholder="Lead UUID"
              aria-label="Lead UUID"
              aria-describedby="basic-addon2"
            />
            <button id="btnSubmit" class="input-group-text btn-outline-success">
              Find Nearby Agents
            </button>
            <button
              id="triggerModalAgents"
              class="input-group-text btn-outline-secondary hidden"
            >
              List View
            </button>
          </div>
          <br />
        </div>
        <table id="tableau" class="table table-hover hidden">
          <thead>
            <tr>
              <th scope="col">Lead Topic</th>
              <th scope="col">Full Name</th>
              <th scope="col">Latitude</th>
              <th scope="col">Longitude</th>
            </tr>
          </thead>
          <tbody id="tableContent"></tbody>
        </table>
        <div id="map" class="d-flex justify-content-between"></div>
      </div>
    </div>
  </div>
</main>

<!-- Agents List Modal -->
<div
  class="modal fade"
  id="modalAgents"
  tabindex="-1"
  role="dialog"
  aria-labelledby="modalAgentsLabel"
>
  <div class="modal-dialog draggable-ui" role="document">
    <div class="modal-content">
      <div class="modal-header no-border">
        <button
          type="button"
          class="btn-close dismissModal"
          aria-label="Close"
        ></button>
      </div>
      <input
        id="search"
        type="text"
        class="form-control disabled"
        placeholder="Begin Typing to filter agents"
        aria-label="search"
        aria-describedby="basic-addon2"
      />
      <div class="modal-body">
        <div id="tableAgentsListView">
          <table class="table table-hover">
            <thead>
              <tr>
                <th></th>
                <th>Agent Name</th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td><input class="form-check-input" type="checkbox" value="" id="flexCheckDefault"></td>
                <td>
                  <a
                    href="https://msvproperties.crm.dynamics.com/main.aspx?appid=6a624849-4115-eb11-a813-000d3a1029e4&amp;forceUCI=1&amp;pagetype=entityrecord&amp;etn=cr4f2_agentsandrealtor&amp;id=6cf64ebf-1dad-eb11-8236-000d3a8bc35c"
                    target="_blank"
                    style="outline: currentcolor none medium"
                    >Wynn Moser</a
                  >
                </td>
                <td>
                  <a
                    href="https://www.google.com/maps/dir/61.204832,-149.926413/61.211513,-149.914719"
                    target="_blank"
                    ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-alarm-fill" viewBox="0 0 16 16">
                      <path d="M6 .5a.5.5 0 0 1 .5-.5h3a.5.5 0 0 1 0 1H9v1.07a7.001 7.001 0 0 1 3.274 12.474l.601.602a.5.5 0 0 1-.707.708l-.746-.746A6.97 6.97 0 0 1 8 16a6.97 6.97 0 0 1-3.422-.892l-.746.746a.5.5 0 0 1-.707-.708l.602-.602A7.001 7.001 0 0 1 7 2.07V1h-.5A.5.5 0 0 1 6 .5zm2.5 5a.5.5 0 0 0-1 0v3.362l-1.429 2.38a.5.5 0 1 0 .858.515l1.5-2.5A.5.5 0 0 0 8.5 9V5.5zM.86 5.387A2.5 2.5 0 1 1 4.387 1.86 8.035 8.035 0 0 0 .86 5.387zM11.613 1.86a2.5 2.5 0 1 1 3.527 3.527 8.035 8.035 0 0 0-3.527-3.527z"/>
                    </svg></a
                  >
                </td>
              </tr>
              <tr>
                <td><input class="form-check-input" type="checkbox" value="" id="flexCheckDefault"></td>
                <td>
                  <a
                    href="https://msvproperties.crm.dynamics.com/main.aspx?appid=6a624849-4115-eb11-a813-000d3a1029e4&amp;forceUCI=1&amp;pagetype=entityrecord&amp;etn=cr4f2_agentsandrealtor&amp;id=720db2c0-ecac-eb11-8236-000d3a8bc535"
                    target="_blank"
                    >Win Moser</a
                  >
                </td>
                <td>
                  <a
                    href="https://www.google.com/maps/dir/61.204832,-149.926413/61.211513,-149.914719"
                    target="_blank"
                    >1626 W 11TH AVE</a
                  >
                </td>
              </tr>
              <tr>
                <td><input class="form-check-input agent-selector" type="checkbox" value="" data-id="{id}"></td>
                <td>
                  <a
                    href="{hotlink}"
                    target="_blank"
                    >{name}</a
                  >
                </td>
                <td>
                  <a
                    href="{map_link}"
                    target="_blank"
                    >{address}</a
                  >

                  <a
                    class="email-single"
                    data-id="{id}
                    >{icon}</a
                  >
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
